                <owl:someValuesFrom rdf:resource="https://www.city.ac.uk/ds/inm713/zacharias_detorakis#State"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An entity to hold all the details of the addres:
- address (i.e. representing the address line)
- city
- country
- state and
- postcode</rdfs:comment>
        <rdfs:label xml:lang="en">Address</rdfs:label>
        <creator xml:lang="en">zacharias.detorakis</creator>
    </owl:Class>
    


